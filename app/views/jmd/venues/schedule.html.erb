<% provide(:title, "Vorspielplan: #{@venue.name}") %>

<h3><%= @contest.name %></h3>

<% other_venues = @contest.venues.select { |v| v != @venue } %>
<% if !other_venues.empty? %>
  <p>
  <% other_venues.each do |venue| %>
    <%= link_to "Zu Vorspielraum \"#{venue.name}\" wechseln", schedule_jmd_contest_venue_path(@contest, venue), class: "btn" %>
  <% end %>
  </p>
<% end %>

<%= render 'draggable_timetables' %>
